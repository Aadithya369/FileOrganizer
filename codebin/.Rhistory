# Define class intervals and frequencies
class_intervals <- seq(170, 250, 10)
midpoints <- seq(175, 245, 10)  # Midpoints of class intervals
frequencies <- c(52, 68, 85, 92, 100, 95, 70, 28)

# Compute Mean
mean_value <- sum(midpoints * frequencies) / sum(frequencies)

# Compute Cumulative Frequency for Median
cumulative_freq <- cumsum(frequencies)
total_freq <- sum(frequencies)
median_class_index <- min(which(cumulative_freq >= total_freq / 2))
h <- 10  # Class width
f_median <- frequencies[median_class_index]
c_median <- cumulative_freq[median_class_index - 1]
l_median <- midpoints[median_class_index] - h / 2
median_value <- l_median + ((total_freq / 2 - c_median) / f_median) * h

# Compute Mode
modal_index <- which(frequencies == max(frequencies))
fm <- frequencies[modal_index]
f1 <- frequencies[modal_index - 1]
f2 <- frequencies[modal_index + 1]
l_mode <- midpoints[modal_index] - h / 2
mode_value <- l_mode + ((fm - f1) / (2 * fm - f1 - f2)) * h

# Compute Quartile Deviation
Q1_index <- min(which(cumulative_freq >= total_freq / 4))
Q3_index <- min(which(cumulative_freq >= 3 * total_freq / 4))
l_Q1 <- midpoints[Q1_index] - h / 2
l_Q3 <- midpoints[Q3_index] - h / 2
f_Q1 <- frequencies[Q1_index]
f_Q3 <- frequencies[Q3_index]
c_Q1 <- cumulative_freq[Q1_index - 1]
c_Q3 <- cumulative_freq[Q3_index - 1]
Q1 <- l_Q1 + ((total_freq / 4 - c_Q1) / f_Q1) * h
Q3 <- l_Q3 + ((3 * total_freq / 4 - c_Q3) / f_Q3) * h
quartile_deviation <- (Q3 - Q1) / 2

# Compute Variance and Standard Deviation
mean_diff_sq <- (midpoints - mean_value) ^ 2
variance <- sum(mean_diff_sq * frequencies) / sum(frequencies)
std_deviation <- sqrt(variance)

# Compute Coefficient of Variation
coefficient_variation <- (std_deviation / mean_value) * 100

# Compute Moments About the Mean
moments <- sapply(1:4, function(k) sum(((midpoints - mean_value) ^ k) * frequencies) / sum(frequencies))

# Compute Pearson’s Coefficients
pearson_coeff_1 <- (mean_value - mode_value) / std_deviation
pearson_coeff_2 <- 3 * (mean_value - median_value) / std_deviation

# Display Results
cat("Mean:", mean_value, "\n")
cat("Median:", median_value, "\n")
cat("Mode:", mode_value, "\n")
cat("Quartile Deviation:", quartile_deviation, "\n")
cat("Variance:", variance, "\n")
cat("Standard Deviation:", std_deviation, "\n")
cat("Coefficient of Variation:", coefficient_variation, "%\n")
cat("First Four Moments About the Mean:", moments, "\n")
cat("Pearson’s Coefficient 1:", pearson_coeff_1, "\n")
cat("Pearson’s Coefficient 2:", pearson_coeff_2, "\n")

