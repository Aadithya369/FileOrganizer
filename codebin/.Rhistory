wages <- c(175, 185, 195, 205, 215, 225, 235, 245)
freq <- c(52, 68, 85, 92, 100, 95, 70, 28)
midpoints <- wages
n <- sum(freq)
mean_val <- sum(midpoints * freq) / n
median_val <- median(rep(midpoints, freq))
mode_val <- midpoints[which.max(freq)]
q1 <- quantile(rep(midpoints, freq), 0.25)
q3 <- quantile(rep(midpoints, freq), 0.75)
quartile_dev <- (q3 - q1) / 2
var_val <- sum(freq * (midpoints - mean_val)^2) / (n - 1)
sd_val <- sqrt(var_val)
cv_val <- (sd_val / mean_val) * 100
m1 <- mean(rep(midpoints, freq))
m2 <- mean((rep(midpoints, freq) - mean_val)^2)
m3 <- mean((rep(midpoints, freq) - mean_val)^3)
m4 <- mean((rep(midpoints, freq) - mean_val)^4)
b1 <- (m3^2) / (m2^3)
b2 <- m4 / (m2^2)
results <- list(mean = mean_val, median = median_val, mode = mode_val, quartile_deviation = quartile_dev, variance = var_val, standard_deviation = sd_val, coefficient_of_variation = cv_val, moments = c(m1, m2, m3, m4), pearson_coefficients = c(b1, b2))
print(results)

